#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <iostream>
#include <vector>
using namespace std;

int main()
{
	int c,a,l;
	int count=1;
	vector<int> aa,bb,cc;
	aa.push_back(0);
	while(cin>>c && c!=-1)
	{
		aa.push_back(c);
	}
	
	cout <<aa.size()<<endl;
	cout<<(aa.size()-1)/2;
	
	
	do{
		int b;
		bb=aa;
		b=(aa.size()-1)/2;
		for(int i=b;i>0;i--)
		{
			if(aa[i+1]>aa[i-1])
			{
				if(aa[i]>aa[i+1])
				{
				swap(aa[i], aa[i+1]);
				}
			}
			else
			{
				if(aa[i]>aa[i-1])
				{
				swap(aa[i], aa[i-1]);
				}	
			}
			
		}
		cc.push_back(aa[1]);
		aa.clear();
		for(int i=b-1;i>0;i--)
		{
			if(i==1)
			continue;
			aa.push_back(bb[i]);
		}	
		
	}while(aa.size()!=1);


	 for(int i=0 ;i<cc.size();i++)
	 {
	 	cout <<cc[i]<<"\t";
	 }

	return 0;
}
